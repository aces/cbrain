<h2>New feedback</h2>

<div class = "generalcontent">

<div class = "generalbox">
<% form_for(@feedback) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :summary %><br />
    <%= f.text_field :summary %>
  </p>
  <p>
    <%= f.label :details %><br />
    <%= f.text_area :details %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', feedbacks_path %>
</div>
</div>